{
  "author": "Guy Ellis <wildfiction@gmail.com>",
  "bugs": {
    "url": "https://github.com/guyellis/nth-day/issues"
  },
  "dependencies": {
    "moment": "2.18.1"
  },
  "description": "Find the nth day (e.g. 3rd Tuesday) in any month",
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-stage-1": "^6.1.2",
    "eslint": "^3.0.0",
    "eslint-config-airbnb": "15.0.2",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.x",
    "eslint-plugin-react": "7.1.0",
    "eslint-plugin-security": "1.4.0",
    "istanbul": "^1.0.0-alpha",
    "mocha": "^3.0.1",
    "pre-commit": "1.2.2",
    "rimraf": "2.6.1"
  },
  "engines": {
    "node": ">=4.2.3"
  },
  "homepage": "https://github.com/guyellis/nth-day#readme",
  "keywords": [
    "date",
    "utility",
    "nth",
    "day"
  ],
  "license": "MIT",
  "main": "lib/nth-day.js",
  "name": "nth-day",
  "pre-commit": {
    "colors": true,
    "run": [
      "lint",
      "test"
    ],
    "silent": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guyellis/nth-day.git"
  },
  "scripts": {
    "build": ". scripts/build.sh",
    "check-coverage": "istanbul check-coverage --statements 0 --branches 0 --functions 0 --lines 0 ./coverage/coverage.json",
    "coverage": "babel-node ./node_modules/istanbul/lib/cli cover node_modules/mocha/bin/_mocha -- test/**/*.js --recursive",
    "lint": "eslint --ext .js .",
    "lintfix": "eslint --fix --ext .js .",
    "mocha": "babel-node node_modules/mocha/bin/_mocha test/**/*.js --recursive",
    "prepublishOnly": "npm run build",
    "test": "npm run coverage"
  },
  "version": "1.1.4"
}
