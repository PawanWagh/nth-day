{
  "lines": 99,
  "statements": 99,
  "functions": 99,
  "branches":90,
  "exclude": [
    "lib/**",
    "node_modules/**",
    "test/**",
    ".vscode/**",
    "scripts/**",
    "coverage/**"
  ],
  "reporter": [
    "lcov",
    "text-summary"
  ],
  "extension": [
    ".js"
  ],
  "cache": true,
  "all": true,
  "check-coverage": true
}
